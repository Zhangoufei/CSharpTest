<UserControl x:Class="TestProgram.Model.QRCodeTest"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestProgram.Model"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="127" Margin="34,165,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="261" VerticalScrollBarVisibility="Visible"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="36,102,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="34,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="34,133,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="158" TextChanged="textBox2_TextChanged" Opacity="0"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="54,30,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="32" Width="138" TextInput="textBlock_TextInput"/>
    </Grid>
</UserControl>
