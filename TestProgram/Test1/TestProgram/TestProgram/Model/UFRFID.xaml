<UserControl x:Class="TestProgram.Model.UFRFID"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TestProgram.Model"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
     >
    <UserControl.Background>
        <ImageBrush ImageSource="/Image/NBackGround.png"></ImageBrush>
    </UserControl.Background>
    <Grid>
        <Button x:Name="button" Content="打开端口" HorizontalAlignment="Left" Margin="39,72,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="textBlock" Text="{Binding TitleCommond}" HorizontalAlignment="Left" Margin="22,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="218" Height="27"/>
        <StackPanel x:Name="atest" HorizontalAlignment="Left" Height="44" Margin="165,50,0,0" VerticalAlignment="Top" Width="115" Visibility="Hidden"  >
            <Button x:Name="button1" Content="Button"/>
        </StackPanel>
        <TextBox HorizontalAlignment="Left" Height="116" Margin="30,99,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="250" Text="{Binding TestBingding}" />
    </Grid>
</UserControl>
