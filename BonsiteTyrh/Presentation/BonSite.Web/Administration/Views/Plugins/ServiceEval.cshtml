@{
    ViewBag.Title = "List";
}

@section body{
    @using (Html.BeginForm("List", "FriendLink"))
{
     <div class="main">
<div class="nav">当前位置：
     <span> &rsaquo; </span> <a href="#">系统管理</a> 
     <span> &rsaquo; </span> <a href="#">服务评价管理</a> 
</div>

        <ul class="submenu">
        	<li class="sel"><a href="#">服务评价管理</a></li>
        </ul>


    <div class="top15">
        	<table width="100%" cellpadding="5" cellspacing="0" class="tblist">
            	<tr>
                    <th width="40" align="center">序号</th>
                    <th width="90" align="left" >姓名</th>
                    <th width="90" align="left" >联系方式</th>
                    <th  width="90" align="left" >地区</th>
                    <th width="50" align="left" >送货人员</th>  
                    <th width="40" align="left" >产品</th>  
                    <th width="40" align="left" >物流</th>  
                    <th width="40" align="left" >服务</th>  
	                <th align="left">意见</th>  
	                <th width="90" align="left">时间</th>  
                </tr>
                @foreach (DataRow row in Model.DataList.Rows)
  {
  <tr>
    <td align="center">@row["ID"]</td>
    <td>@row["Name"]</td>
    <td>@row["Contact"]</td>
    <td>@row["Province"] @row["City"]</td>
    <td>@row["Courier"]</td>
    <td>@row["EvalProduct"]</td>
    <td>@row["EvalLogistics"]</td>
    <td>@row["EvalService"]</td>
    <td>@row["Body"]</td>
    <td>@TypeHelper.ObjectToDateTime(row["CreateTime"].ToString()).ToString("yyyy-MM-dd") </td>
   
    </tr>
  }
  
                </table>
        <div class="dataListEdit">
</div>
</div>
         
         </div>
    
    }
}